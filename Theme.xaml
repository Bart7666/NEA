<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Contains the list of all controls with a key associated with each so styles can be changed easily -->
    <!-- To add more controls create master key alongside style here and actual styles in ThemesList Dictionary -->
    <Style TargetType="MenuItem" x:Key="MenuItem">
    </Style>
    <Style TargetType="Button" x:Key="Button">
    </Style>
    <Style TargetType="Menu" x:Key="Menu">
        <Setter Property="Background" Value="White"/>
    </Style>
    <Style TargetType="Window" x:Key="Window">
    <!-- This is used to control the background to each window of the solution -->
    </Style>
    <Style TargetType="TextBlock" x:Key="TextBlock">
    </Style>
    <Style TargetType="TextBox" x:Key="TextBox">
    </Style>
    <Style TargetType="Label" x:Key="Label">
    </Style>
    <Style TargetType="RadioButton" x:Key="RadioButton">
    </Style>
    
</ResourceDictionary>
    